{
  "name": "webworldwind-x",
  "version": "2.2.2",
  "description": "Extension repository collecting work done in the scope of EOAPPS project within ESA.",
  "main": "dist/main.js",
  "files": [
    "dist/main.js",
    "dist/main.js.map"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run jsdoc && npm run webpack && npm run copy && npm run remove",
    "build:watch": "parallelshell 'npm run webpack:watch' 'npm run examples:watch'",
    "clean": "del-cli dist",
    "copy": "copyfiles examples/*.html dist/ && copyfiles -u 1 dist/*Example.js* dist/examples/ && copyfiles examples/*.png dist/ && copyfiles -u 1 examples/images/* dist/examples && copyfiles -u 1 examples/images/view/* dist/examples && copyfiles index.html dist/ && copyfiles examples/*.css dist/ && copyfiles examples/data/* dist/",
    "examples:watch": "watch 'npm run build' ./examples",
    "jsdoc": "jsdoc --destination dist/api-doc -r src",
    "lint": "jshint ./src ",
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "release": "npm run build && npm run version:minor && npm publish",
    "remove": "del-cli dist/*Example.js*",
    "serve": "http-server ./dist/",
    "start": "npm run build && npm run serve",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "webpack": "webpack-cli --config webpack.config.js --mode production",
    "webpack:watch": "webpack-cli --config webpack-dev.config.js -w --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balhar-jakub/webworldwind-x.git"
  },
  "keywords": [
    "Web World Wind",
    "Earth Observation",
    "3D Globe"
  ],
  "author": "Jakub Balhar, Yann Voumard, Florin Draghici, Servet Giafher, Claudia Ifrim",
  "license": "APL-2.0",
  "bugs": {
    "url": "https://github.com/balhar-jakub/webworldwind-x/issues"
  },
  "homepage": "https://github.com/balhar-jakub/webworldwind-x#readme",
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "babel-loader": "8.0.0",
    "babel-polyfill": "^6.26.0",
    "copyfiles": "2.1.0",
    "css-loader": "1.0.0",
    "del-cli": "1.1.0",
    "http-server": "0.11.1",
    "jasmine": "3.2.0",
    "jsdoc": "3.5.5",
    "jshint": "2.10.2",
    "karma": "3.0.0",
    "karma-babel-preprocessor": "8.0.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.5",
    "parallelshell": "3.0.2",
    "puppeteer": "^1.19.0",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "watch": "1.0.2",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "easysax": "^0.3.2",
    "webworldwind-esa": "0.10.0-alpha.4",
    "xml-js": "^1.6.11"
  }
}
